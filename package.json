    "prettier": "^3.6.2",
    "typescript": "^4.9.5",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.scss": [
      "prettier --write"
    ]
  }
